<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Data ID</th>
      <th>Time</th>
      <th>User</th>
      <th>Longitude</th>
      <th>Latitude</th>
    </tr>
  </thead>

  <tbody>
    <% @locations.each do |location| %>
      <tr>
      	<td><%= location.id %></td>
      	<td><%= location.time %></td>
        <td><%= location.user_id %></td>
        <td><%= location.longitude %></td>
        <td><%= location.latitude %></td>
      </tr>
    <% end %>
  </tbody>
</table>